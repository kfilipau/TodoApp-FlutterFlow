{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp of the user's birthdate in UTC."
    },
    "created_time": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp of when the user was created in UTC."
    },
    "display_name": {
      "type": "string",
      "description": "Display name of the user."
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email address of the user."
    },
    "photo_url": {
      "type": "string",
      "format": "uri",
      "description": "URL to the user's profile picture."
    },
    "uid": {
      "type": "string",
      "description": "Unique identifier for the user."
    },
    "phone_number": {
      "type": ["string", "null"],
      "description": "Phone number of the user (optional)."
    }
  },
  "required": ["birthdate", "created_time", "display_name", "email", "uid"]
}
